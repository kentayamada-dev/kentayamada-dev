{
  "type": "module",
  "engines": {
    "node": "22.x",
    "pnpm": "10.x",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "eslint": "eslint ./src/",
    "eslint-inspector": "eslint --inspect-config",
    "prettier": "prettier . --write",
    "knip": "knip --debug",
    "check": "pnpm test && pnpm knip && pnpm prettier && pnpm eslint && pnpm tsc",
    "build-next": "bash -c 'until next build; do echo \"Build failed, retrying...\"; done'",
    "build-storybook": "storybook build --output-dir ./.storybook/output",
    "dev-next": "next dev --port $NEXTJS_PORT --experimental-https --experimental-https-key ~/mkcert/localhost-key.pem --experimental-https-cert ~/mkcert/localhost.pem",
    "dev-storybook": "storybook dev --https --port $STORYBOOK_PORT --host 127.0.0.1 --no-open --no-version-updates --ssl-cert ~/mkcert/localhost.pem --ssl-key ~/mkcert/localhost-key.pem",
    "start": "next start --port $NEXTJS_PORT",
    "test": "vitest run",
    "update-dep": "ncu --interactive --peer",
    "prepare": "cd .. && mkdir --parents ./portfolio/.husky && husky ./portfolio/.husky && echo 'chmod +x ./portfolio/husky-pre-commit.sh && ./portfolio/husky-pre-commit.sh' > ./portfolio/.husky/_/pre-commit"
  },
  "dependencies": {
    "@bprogress/next": "3.2.12",
    "@googleapis/sheets": "11.1.0",
    "@headlessui/react": "2.2.7",
    "@number-flow/react": "0.5.10",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tailwindcss/typography": "0.5.16",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "cookies-next": "6.1.0",
    "graphql-tag": "2.12.6",
    "katex": "0.16.22",
    "libphonenumber-js": "1.12.10",
    "lottie-react": "2.4.1",
    "motion": "12.23.12",
    "next": "15.4.5",
    "next-mdx-remote-client": "2.1.3",
    "next-themes": "0.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-google-recaptcha": "3.1.0",
    "react-hook-form": "7.61.1",
    "redis": "5.6.1",
    "rehype-katex": "7.0.1",
    "remark-gfm": "4.0.1",
    "remark-math": "6.0.0",
    "resend": "4.7.0",
    "scroll-hint": "1.2.9",
    "shiki": "3.8.1",
    "tailwindcss": "4.1.11",
    "unist-util-visit": "5.0.0",
    "zod": "4.0.14"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "9.2.0",
    "@eslint/js": "9.32.0",
    "@next/eslint-plugin-next": "15.4.5",
    "@storybook/addon-a11y": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-themes": "8.6.14",
    "@storybook/manager-api": "8.6.14",
    "@storybook/nextjs": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/test": "8.6.14",
    "@stylistic/eslint-plugin": "5.2.2",
    "@tailwindcss/postcss": "4.1.11",
    "@types/node": "24.1.0",
    "@types/react": "19.1.9",
    "@vitest/eslint-plugin": "1.3.4",
    "eslint": "9.32.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-better-tailwindcss": "3.7.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-no-barrel-files": "1.2.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-storybook": "0.12.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "knip": "5.62.0",
    "lint-staged": "16.1.2",
    "npm-check-updates": "18.0.2",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-sort-json": "4.1.1",
    "prettier-plugin-tailwindcss": "0.6.14",
    "storybook": "8.6.14",
    "storybook-dark-mode": "4.0.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.38.0",
    "vitest": "3.2.4"
  }
}
