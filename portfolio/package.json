{
  "type": "module",
  "engines": {
    "pnpm": "9.15.5",
    "node": "22.14.0",
    "npm": "please-use-pnpm"
  },
  "scripts": {
    "eslint": "eslint ./src/",
    "eslint-inspector": "eslint --inspect-config",
    "prettier": "prettier . --write",
    "knip": "knip --debug",
    "check": "pnpm test && pnpm knip && pnpm prettier && pnpm eslint && pnpm tsc",
    "build-next": "bash -c 'until next build; do echo \"Build failed, retrying...\"; done'",
    "build-storybook": "storybook build --output-dir ./public/storybook",
    "build": "pnpm build-storybook && pnpm build-next",
    "dev-next": "next dev --port $NEXTJS_PORT --experimental-https --experimental-https-key ~/mkcert/localhost-key.pem --experimental-https-cert ~/mkcert/localhost.pem",
    "dev-storybook": "storybook dev --https --port $STORYBOOK_PORT --host 127.0.0.1 --no-open --no-version-updates --ssl-cert ~/mkcert/localhost.pem --ssl-key ~/mkcert/localhost-key.pem",
    "start": "next start --port $NEXTJS_PORT",
    "test": "vitest run",
    "update-dep": "ncu --interactive --peer",
    "prepare": "cd .. && mkdir --parents ./portfolio/.husky && husky ./portfolio/.husky && echo 'chmod +x ./portfolio/husky-pre-commit.sh && ./portfolio/husky-pre-commit.sh' > ./portfolio/.husky/_/pre-commit"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@t3-oss/env-nextjs": "0.12.0",
    "@tailwindcss/typography": "0.5.16",
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "cookies-next": "5.1.0",
    "framer-motion": "12.5.0",
    "graphql": "16.10.0",
    "graphql-request": "7.1.2",
    "lottie-react": "2.4.1",
    "next": "15.2.2",
    "next-themes": "0.4.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.54.2",
    "rehype-katex": "7.0.1",
    "rehype-pretty-code": "0.14.0",
    "rehype-react": "8.0.0",
    "remark-gfm": "4.0.1",
    "remark-math": "6.0.0",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.1",
    "scroll-hint": "1.2.9",
    "shiki": "1.29.2",
    "tailwindcss": "4.0.14",
    "unified": "11.0.5",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "8.17.5",
    "@eslint/js": "9.22.0",
    "@next/eslint-plugin-next": "15.2.2",
    "@storybook/addon-a11y": "8.6.4",
    "@storybook/addon-essentials": "8.6.4",
    "@storybook/addon-themes": "8.6.4",
    "@storybook/manager-api": "8.6.11",
    "@storybook/nextjs": "8.6.4",
    "@storybook/react": "8.6.4",
    "@storybook/test": "8.6.4",
    "@stylistic/eslint-plugin": "4.2.0",
    "@tailwindcss/postcss": "4.0.14",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@vitest/eslint-plugin": "1.1.37",
    "eslint": "9.22.0",
    "eslint-import-resolver-typescript": "3.8.5",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-sort-exports": "0.9.1",
    "eslint-plugin-storybook": "0.11.4",
    "eslint-plugin-tailwindcss": "3.18.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "knip": "5.45.0",
    "lint-staged": "15.4.3",
    "npm-check-updates": "17.1.15",
    "postcss": "8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "storybook": "8.6.4",
    "storybook-dark-mode": "4.0.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.26.1",
    "vitest": "3.0.8"
  }
}
